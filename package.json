  "scripts": {
    "start": "yarn start:local",
    "start:local": "env-cmd -f ./.env.local react-scripts start",
    "start:test": "env-cmd -f ./.env.test react-scripts -r @cypress/instrument-cra start",
    "start:ci": "react-scripts -r @cypress/instrument-cra start",
    "build": "yarn build:react",
    "build:react": "react-scripts build",
    "build:local": "env-cmd -f ./.env.local yarn build",
    "build:dev": "env-cmd -f ./.env.dev yarn build",
    "build:prod": "env-cmd -f ./.env.prod yarn build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override -f ./.env.prod yarn build",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "lint": "eslint .",
    "prettier:check": "prettier --check '{src,cypress}/**/*.js'",
    "prettier:write": "prettier --write '{src,cypress}/**/*.js'",
    "pre-commit": "yarn prettier:check && yarn lint",
    "eject": "react-scripts eject",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "cypress:open": "env-cmd -f ./.env.test cypress open",
    "cypress": "concurrently \"yarn start:test\" \"wait-on http://localhost:3334 && yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --headless --browser firefox --spec \"cypress/**/*.cy.js\"",
    "release:patch": "standard-version -a --release-as patch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "stream": false,
    "zlib": false
  },
  "packageManager": "yarn@3.2.0",
  "release": {
    "analyzeCommits": "conventional"
  },
  "devDependencies": {
    "@babel/eslint-parser": "~7.19.1",
    "@babel/preset-react": "~7.18.6",
    "@commitlint/cli": "~17.1.2",
